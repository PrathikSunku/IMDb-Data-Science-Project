    "# Converting CVotes to int type\n",
    "\n",
    "cvotes_list_col = []\n",
    "for i in genre_top10.columns:\n",
    "    if i.startswith('CVotes'):\n",
    "        cvotes_list_col.append(i)\n",
    "\n",
    "genre_top10[cvotes_list_col] = genre_top10[cvotes_list_col].astype('int')\n",
    "genre_top10.head()"
