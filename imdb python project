    "# Divide the 'gross' and 'budget' columns by 1000000 to convert '$' to 'million $'\n",
    "\n",
    "movies['Gross']=movies['Gross'].floordiv(1000000)\n",
    "movies['budget']=movies['budget'].floordiv(1000000)\n",
    "movies\n",
    "\n"
   ]
